		<li data-step="7"{% if step_enabled == 7 %} class="active"{% elseif step_enabled > 7 %} class="complete"{% endif %}>
		<span class="step">7</span>
		<span class="title">{{ 'home.step_7'|trans }}</span>
	  </li>

		<li data-step="8"{% if step_enabled == 8 %} class="active"{% elseif step_enabled > 8 %} class="complete"{% endif %}>
